Ans:5
unction email(str)
{
var EmailFormat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/; 
if(EmailFormat.test(str))
{ 
console.log("Valid email address!"); 
} 
else
{ 
console.log("You have entered an invalid email address!"); 
}
}
email('me-info@example.com');
